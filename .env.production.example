# Fantasy Football Analyzer - Production Environment Variables
# Copy to .env.production and configure with your values

# Application Configuration
VITE_APP_NAME=Fantasy Football Analyzer
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=production

# API Configuration (Set in Netlify Dashboard for security)
# VITE_FANTASYDATA_API_KEY=your_api_key_here_set_in_netlify_dashboard

# Feature Flags
VITE_ENABLE_LIVE_DATA=true
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_DRAFT_SIMULATION=true

# Performance Settings
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_ANALYTICS=true

# Development/Debug (set to false for production)
VITE_DEBUG_MODE=false
VITE_ENABLE_DEVTOOLS=false