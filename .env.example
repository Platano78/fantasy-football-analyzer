# Fantasy Football Analyzer - Environment Variables

# Application Settings
VITE_APP_NAME=Fantasy Football Analyzer
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Advanced Fantasy Football Draft Tool with AI Analysis

# API Configuration
VITE_API_BASE_URL=https://api.fantasyfootballanalyzer.com
VITE_MCP_ENDPOINT=ws://localhost:8080/mcp

# Feature Flags
VITE_ENABLE_LIVE_DATA=true
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_DRAFT_SIMULATION=true
VITE_ENABLE_ANALYTICS=true

# Analytics & Monitoring
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_POSTHOG_KEY=

# Performance Settings
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_SHOW_PERFORMANCE_STATS=false